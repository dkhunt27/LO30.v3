@model LO30.Data.Objects.Setting

@{
  ViewBag.Title = "Admin | Settings | Details";
}

<h2>Details</h2>

@if (!string.IsNullOrEmpty(ViewBag.ErrorMessage))
{
  <div class="alert alert-danger" role="alert">@ViewBag.ErrorMessage</div>
  ViewBag.ErrorMessage = "";
}

<fieldset>
  <legend>Setting</legend>

  <dl class="dl-horizontal">
    <dt>
      @Html.LabelFor(model => model.SettingId)
    </dt>
    <dd class="clearfix">
      @Html.DisplayFor(model => model.SettingId)
    </dd>

    <dt>
      @Html.LabelFor(model => model.SettingName)
    </dt>
    <dd class="clearfix">
      @Html.DisplayFor(model => model.SettingName)
    </dd>

    <dt>
      @Html.LabelFor(model => model.SettingValue)
    </dt>
    <dd class="clearfix">
      @Html.DisplayFor(model => model.SettingValue)
    </dd>
  </dl>
</fieldset>
<p>
  @Html.ActionLink("Edit", "Edit", new { id = Model.SettingId }) |
  @Html.ActionLink("Back to List", "List")
</p>
